@keyframes icon-spin {
  0% {
    -webkit-transform: rotate(0);
    transform: rotate(0);
  }

  100% {
    -webkit-transform: rotate(359deg);
    transform: rotate(359deg);
  }
}

.iconfont-spin {
  -webkit-animation: icon-spin 2s infinite linear;
  animation: icon-spin 2s infinite linear;
  display: inline-block;
}

.iconfont-pulse {
  -webkit-animation: icon-spin 1s infinite steps(8);
  animation: icon-spin 1s infinite steps(8);
  display: inline-block;
}

@font-face {
  font-family: 'iconfont';  /* project id 1127277 */
  src: url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.eot');
  src: url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.woff2') format('woff2'),
  url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.woff') format('woff'),
  url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_1127277_x223gvlnfmd.svg#iconfont') format('svg');
}

.iconfont {
  font-family: iconfont !important;
  font-size: inherit;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

